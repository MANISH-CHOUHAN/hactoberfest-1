#include <iostream>

using namespace std;

void pascal(int n)
{
    for (int i = 1; i <= n; i++)
    {                                           //for loop
        int c = 1;
        for (int j = 1; j <= i; j++)
        {
            cout<<c<<" ";
            c = c * (i-j)/j;
        }                                        
        cout<<endl;
    }
    
}
    int main()                        //main function
    {
        pascal(7);
        return 0;
    }
